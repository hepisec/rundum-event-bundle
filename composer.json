{
    "name": "rundum/event-bundle",
    "description": "Event bundle for Symfony",
    "type": "symfony-bundle",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Rundum\\EventBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Rundum\\EventBundle\\Tests\\": "tests/"
        }
    },    
    "authors": [
        {
            "name": "Hendrik Pilz",
            "email": "hepisec@gmail.com"
        }
    ],
    "require": {
        "symfony/http-kernel": "^6.3",
        "symfony/framework-bundle": "^6.3",
        "doctrine/orm": "^2.16",
        "doctrine/doctrine-bundle": "^2.10"
    },
    "require-dev": {
        "symfony/maker-bundle": "^1.50",
        "symfony/phpunit-bridge": "^6.3",
        "composer/semver": "^3.3",
        "nyholm/symfony-bundle-test": "^2.0"
    }
}
